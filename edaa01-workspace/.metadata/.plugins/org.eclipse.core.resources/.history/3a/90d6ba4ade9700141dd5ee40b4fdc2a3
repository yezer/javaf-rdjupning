package bst;

public class ExamQ {
	/**
	* Retrieves and removes the first (lowest) element, or returns null if this
	* tree is empty.
	* @return first (lowest) element, or null if this tree is empty
	*/
	public E pollFirst() {
	if (root == null) {
	return null;
	}
	E temp = null;
	if (root.left == null) {
	temp = root.element;
	root = root.right;
	} else {
	Node<E> pre = root;
	Node<E> p = root.left;
	while (p.left != null) {
	pre = p;
	p = p.left;
	}
	temp = p.element;
	pre.left = p.right;
	}
	return temp;
	}
}
